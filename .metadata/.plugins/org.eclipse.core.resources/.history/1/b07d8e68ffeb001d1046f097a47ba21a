package dao;

import java.sql.PreparedStatement;
import java.sql.ResultSet;

import model.Food;
import model.Table;

public class FoodDao extends DAO  {
	public FoodDao() {
		super();
	}
	public boolean searchFood(String foodName){
		String sql = "SELECT * FROM orderfood.food WHERE foodName LIKE ?";
		try{
		
			PreparedStatement ps = con.prepareStatement(sql);
			ps.setString(1, "%" + foodName + "%");
			ResultSet rs = ps.executeQuery();
			Food rm = new Food();
			while(rs.next()){
				
				
				System.out.println("Ket qua la");
				System.out.println( rm.getFoodName());
								
			}
		}catch(Exception e){
			e.printStackTrace();
			return false;
		}
		return true;	
		
	}
}
